{
  "autorun": false,
  "description": "Controls automatic progression between frontend generation phases",
  "phases": {
    "Phase 1": {
      "name": "Frontend Requirements Analysis",
      "agent": "frontend-requirements-analysis",
      "estimatedDuration": "2-3 hours"
    },
    "Phase 2": {
      "name": "UI/UX Design and Component Architecture",
      "agent": "frontend-ui-ux-design",
      "estimatedDuration": "3-4 hours"
    },
    "Phase 3": {
      "name": "Data Layer and State Management",
      "agent": "frontend-data-layer",
      "estimatedDuration": "4-5 hours"
    },
    "Phase 4": {
      "name": "Component Implementation (TDD)",
      "agent": "frontend-component-implementation",
      "estimatedDuration": "6-8 hours"
    },
    "Phase 5": {
      "name": "Integration and Testing",
      "agent": "frontend-integration-testing",
      "estimatedDuration": "4-5 hours"
    }
  },
  "settings": {
    "stopOnError": true,
    "requireManualApproval": true,
    "notifyOnCompletion": true
  }
}
