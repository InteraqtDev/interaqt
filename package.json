{
  "name": "interaqt",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.1",
    "@types/acorn": "^6.0.4",
    "@types/better-sqlite3": "^7.6.12",
    "@types/connect": "^3.4.38",
    "@types/cors": "^2.8.17",
    "@types/node": "^22.13.10",
    "@types/pg": "^8.11.11",
    "@vitest/coverage-v8": "^3.1.2",
    "release-it": "^19.0.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vite": "^7.1.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "check": "tsc --noEmit --skipLibCheck",
    "check:all": "npm run check:shared && npm run check:runtime && npm run check:storage",
    "check:shared": "tsc --project tsconfig.shared.json",
    "check:runtime": "tsc --project tsconfig.runtime.json",
    "check:storage": "tsc --project tsconfig.storage.json",
    "prebuild": "rimraf dist",
    "build": "vite build --config vite.prod.config.ts",
    "postbuild": "npm run check:build",
    "check:build": "tsc --noEmit --skipLibCheck -p tsconfig.prod.json",
    "test": "vitest run",
    "test:runtime": "vitest run tests/runtime",
    "test:storage": "vitest run tests/storage",
    "test:shared": "vitest run tests/shared",
    "release": "release-it"
  },
  "version": "0.1.0",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "author": "sskyy<skyking_H@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "@electric-sql/pglite": "^0.3.2",
    "acorn": "^8.14.1",
    "better-sqlite3": "^11.8.1",
    "chalk": "^5.4.1",
    "mysql2": "^3.13.0",
    "pg": "^8.14.0",
    "uuidv7": "^1.0.2",
    "winston": "^3.17.0"
  },
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "release-it": {
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "conventionalcommits"
      }
    },
    "git": {
      "commitMessage": "chore(release): v${version}",
      "tagName": "v${version}",
      "push": true,
      "requireCleanWorkingDir": true
    },
    "npm": {
      "publish": true
    },
    "hooks": {
      "before:init": "npm test && npm run build"
    }
  }
}
